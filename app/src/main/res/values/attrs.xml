<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleProgressBar">
        <attr name="antiAlias" />
        <attr name="startAngle" />
        <attr name="sweepAngle" />
        <attr name="animTime" />
        <attr name="maxValue" />
        <attr name="unit" />
        <attr name="unitSize" />
        <attr name="unitColor" />
        <attr name="hint" />
        <attr name="hintSize" />
        <attr name="hintColor" />
        <attr name="precision" />
        <attr name="value" />
        <attr name="valueSize" />
        <attr name="valueColor" />
        <attr name="valueUnitSize" />
        <attr name="bgArcColor" />
        <attr name="pointerColor" />
        <attr name="arcWidth" />
        <attr name="arcColors" />
        <attr name="textOffsetPercentInRadius" />
        <attr name="bgArcWidth" />
        <attr name="dottedLineCount" />
        <attr name="lineDistance" />
        <attr name="dottedLineWidth" />
        <attr name="foreStartColor" />
        <attr name="foreEndColor" />
        <attr name="drawbleIcon" />
    </declare-styleable>

    <declare-styleable name="GradientView">
        <attr name="from" format="color" />
        <attr name="to" format="color" />
        <attr name="direct" format="string" />
    </declare-styleable>


    <declare-styleable name="VerticalSeekBar">
        <attr name="seekBarWidth" format="dimension"></attr>
        <attr name="seekBarHeight" format="dimension"></attr>
        <attr name="seekBgWidth" format="dimension"></attr>
        <attr name="seekBgHeight" format="dimension"></attr>

    </declare-styleable>

    <declare-styleable name="RealtimeBlurView">
        <attr format="dimension" name="realtimeBlurRadius"/>
        <attr format="float" name="realtimeDownsampleFactor"/>
        <attr format="color" name="realtimeOverlayColor"/>
    </declare-styleable>

    <declare-styleable name="AirCircleProgressView">
        <attr name="color_normal" format="color" />
        <attr name="color_select" format="color" />
    </declare-styleable>

</resources>
